div[class^='content'] .bui-stdmod-footer button:first-child{background-color:#6BC57D;color:#fff;border-color:#58B26A;}
div[class^='content'] .bui-dialog .bui-stdmod-footer{background: linear-gradient(#FEFEFE,#EDEDE7);background: -ms-linear-gradient(#FEFEFE,#EDEDE7);background: -webkit-linear-gradient(#FEFEFE,#EDEDE7);}
div[class^='content'] .bui-dialog .bui-stdmod-footer .button + .button{margin-left:19px;}
div[class^='content']  input.input-large, textarea.input-large, select.input-large{width: 700px;}
div[class^='content'] .bui-dialog .bui-stdmod-body{padding-top: 50px;}
div[class^='content'] textarea{background:#F8F8F8;}
div[class^='content'] .bui-dialog .bui-stdmod-body{height: 250px;}
input[type].none_border{border: none;border-left: none;border-right: none;border-top: none;border-bottom: none;}
