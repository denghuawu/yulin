/*确定*/
.affirm_w{position:fixed;left:0px;top:0px;z-index:99;width:100%;height:100%;background-color:rgba(66,66,66,0.5);color: #676767;}
.affirm_c{position:absolute;left:0;right:0;top:0;bottom:0;margin:auto;width:36.0rem;height:18.875rem;background-color:#eeeeee;border-radius:1rem;padding:0 1.375rem;}
.affirm_c .t{max-height:10rem;text-align:center;margin-top:2rem;font-size:1.2em;border-bottom:2px solid #f7f7f7; }
.affirm_c .i{text-align:center;position:absolute;bottom:1rem;width:100%;left:0;padding-top:1rem;}
.affirm_c .c{text-align:center;margin-top: 0.625rem;}
.affirm_c .i span{width:16.25rem;border-radius:0.5rem;height:3.5rem;line-height:3.5rem;;display:inline-block;}
.affirm_c .i span.true{background-color:#377812;margin-left:1rem;color:#fff;}
.affirm_c .i span.false{background-color:#fff;border:1px solid #b8d201;color:#b8d201;}


