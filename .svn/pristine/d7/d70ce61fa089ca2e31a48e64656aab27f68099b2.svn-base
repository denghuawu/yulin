/*购物车*/
.shopping {background:#f7f3e7;margin-bottom: 11.25rem;}
.shopping li{margin-bottom:0.625rem;background:#fff;padding:0.9375rem 0;}
.shopping .c,.shopping .i,.shopping .n,.shopping .r{float:left;height:10.3125rem;}
.shopping .c{width:5rem;text-align:center;}
.shopping .c .t span{background:url(../images/plcg.png) 0px -1580px no-repeat;width:40px;height:40px;display:inline-block;margin-top:20px;}
.shopping .c .t span.now{background:url(../images/plcg.png) 0px -1444px no-repeat;}
.shopping .r{width:28.75rem;float:right;display:none;}
.shopping .r .list_n{height:4.3125rem;float:left;padding:3rem 0;font-size:37px;}
.shopping .r .list_n input[type='text']{width:10rem;height:4.125rem;border:1px solid #bab3ae;vertical-align:top;border-radius:0;padding:0 0.625rem;text-align:center;}
.shopping .r .list_n input[type='button']{width:4.375rem;height:4.25rem;border:1px solid #bab3ae;border-width:1px 0px;vertical-align:top;background:#fff;color:#bab3ae;border-radius:0;}
.shopping .list_r{float:right;background-color:#f80;font-size:37px;width:8.25rem;color:#fff;text-align:center;height:10.3125rem;line-height:10.3125rem;}
.shopping .i img{width:10.3125rem;height:10.3125rem;}
.shopping .n{width:25.625rem;position:relative;margin-left:0.875rem;}
.shopping .n .s{width:25.625rem;position:absolute;bottom:0;}
.accounts{position:fixed;width:100%;height:4.75rem;bottom:6.0625rem;background:#fff;border-top:1px solid #888888;line-height:4.75rem;left:0;}
.accounts .total{padding-left:1.25rem;}
.accounts_btn{width:14.375rem;height:4.75rem;display:inline-block;vertical-align:middle;text-align:center;background:#377812;color:#fff;line-height:4.75rem;font-size:1em;}


/*轮播图*/
/*.index_top{position:relative;}*/
.swiper-container{width:100%;height:24.25rem;overflow:hidden;position:relative;}
.swiper-wrapper{width:9999px;height:100%;}
.swiper-slide{overflow:hidden;text-align:center;}
.swiper-slide img{width:100%;height:100%}
.swiper_list{float:left;text-align:center;height:24.25rem;}
.swiper_nav{text-align:center;position:absolute;z-index:1;bottom: 0.5em;width: 100%;}
.swiper_nav .swiper-pagination-switch{width:3.125rem; height:0.3125rem; display:inline-block; background:#fff; background-size:100% 100%; line-height:0px;}
.swiper_nav .swiper-active-switch{background:#cc0d2d;width:5rem;-webkit-transition:all 0.1s;}

/*首页*/
.index{margin-top:-5.5625rem;background:#f7f3e7;}
.index_top{position: relative;}
.index .index_con{padding:0px 0.375rem;}
.index .index_con .new,.index .index_con .hot,.index .index_con .recommend{width:100%;display:inline-block;height:10.6875rem;position:relative;margin-top:0.75rem;}
.index .index_con .new img,.index .index_con .hot img,.index .index_con .recommend img{width:100%;height:100%;}
.index .index_con .new span,.index .index_con .hot span,.index .index_con .recommend span{position:absolute;right:-2px;top:-2px;width:111px;height:95px;background:url(../images/dztj.png) no-repeat;}
.index .index_con .new span{background-position:0px 0px;}
.index .index_con .hot span{background-position:0px -244px;}
