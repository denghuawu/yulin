/*注册*/
.register{padding:6.25rem 5rem;}
.register_con{text-align:center;}
.register_con input[type='text'],.register_con input[type='password']{width:97%;border:1px solid #888888;border-radius:5px;height:3.625rem;padding-left:3%;margin-bottom:1.875rem;background: #f7f7f7;}
.register_con .code{}
