.login_con input[type='text'],.login_con input[type='password']{width:27.5rem;border:1px solid #888888;border-radius:5px;height:3.625rem;padding-left:1.25rem;background: #e6e6e6;border:none;}
.login_con input[type='submit']{width:19.25rem;height:4.375rem;border:none;background-color:#b8d201;color:#fff;border-radius:5px;}
.login_nav a{color:#838282;}

