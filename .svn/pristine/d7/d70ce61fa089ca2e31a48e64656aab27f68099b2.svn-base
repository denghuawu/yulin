.register_con .code input[type='button']{width:35%;border-radius:5px;height:3.75rem;background:#fff;border:1px solid #b0b0b0;float:right;font-size:1.4375rem;}
.register_con input[type='submit']{width:15rem;height:4rem;border:none;background-color:#37780c;color:#fff;border-radius:5px;}

/*登陆*/
.login{padding:4rem 5rem ;}
