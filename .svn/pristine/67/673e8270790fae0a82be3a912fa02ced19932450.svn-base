/*确定*/
.affirm_w{position:fixed;left:0px;top:0px;z-index:99;width:100%;height:100%;background-color:rgba(66,66,66,0.5);color: #676767;}
.affirm_c{position:absolute;left:0;right:0;top:0;bottom:0;margin:auto;width:36.0rem;height:18.875rem;background-color:#eeeeee;border-radius:1rem;padding:0 1.375rem;}
.affirm_c .t{max-height:10rem;text-align:center;margin-top:2rem;font-size:1.2em;border-bottom:2px solid #f7f7f7; }
.affirm_c .i{text-align:center;position:absolute;bottom:1rem;width:100%;left:0;padding-top:1rem;}
.affirm_c .c{text-align:center;margin-top: 0.625rem;}
.affirm_c .i span{width:16.25rem;border-radius:0.5rem;height:3.5rem;line-height:3.5rem;;display:inline-block;}
.affirm_c .i span.true{background-color:#377812;margin-left:1rem;color:#fff;}
.affirm_c .i span.false{background-color:#fff;border:1px solid #b8d201;color:#b8d201;}

/*首页*/
.commodity_search{text-align: center;padding:1.0625rem 0;border-bottom: 1px solid #ddd;}
.commodity_search input[type="text"]{height: 4rem;line-height: 4rem;vertical-align: middle;font-size: 1.625rem;color: #828282;background: #ebebeb;width:26.875rem;text-indent: 1em;border-radius: 0.25rem 0 0 0.25rem;}
.commodity_search input[type="submit"]{height: 4rem;line-height: 4rem;vertical-align: middle;font-size: 1.625rem;width: 8.75rem;border-radius:0 0.25rem 0.25rem 0;background: url(../images/search-btn.png) 0 0 no-repeat;background-size: 100% 100%;}
.commodity_top .nav a{width: 15rem;text-align: center;float: left;}
.commodity_top .nav div{height: 5rem;line-height: 5rem;border-bottom: 1px solid #fff;}
.commodity_top .nav div.opt{color: #ff755b;border-bottom-color: #ff755b;}
.commodity_top .nav div:hover{color: #ff755b;border-bottom-color: #ff755b;}
.commodity_con{background: #ece7e4;padding:0.3125rem;}
.commodity_con>div{background: #fff;margin:0.3125rem;width:21.5625rem;float: left;}
.commodity_con div a>div:first-child{width: 21.5625rem;height: 18.75rem; display: table-cell;text-align: center;vertical-align: middle;border-bottom:1px solid #e0e0e0;}
.commodity_con div img{min-height: 88%;max-height: 98%;max-width: 100%;}
.commodity_con div a>div:last-child{height: 6.875rem;padding:0.625rem 0.625rem 0;}
.commodity_con div a>div:last-child p:first-child{height:4.125rem;}


.swiper-container{height: 24.25rem;width: 100%;overflow: hidden;border-bottom: 1px solid #ddd;}
.swiper-slide{display: table;}
.swiper-slide div{height: 24.25rem;width: 100%;text-align: center;display: table-cell;vertical-align: middle;}
.swiper-slide img{max-width: 100%;max-height: 100%;min-height: 70%;}
.swiper_nav{margin-top: -2.5rem;z-index: 2;position: relative;width: 100%;text-align: center;}
.shop_top .swiper_nav .swiper-pagination-switch {width:1rem;height:1rem;border-radius:50%;margin-right:1rem;display:inline-block; background:#fff;line-height:0px;opacity: 0.5;}
.shop_top .swiper_nav .swiper-active-switch{width:1rem;height:1rem;border-radius:50%;background:#ff745a;-webkit-transition:all 0.1s;opacity: 0.5;}


/*商品内容页*/
.shop_btn{height: 5rem;}
.shop_btn_con{position:fixed;bottom:0;height:5rem;width:100%;background-color:#fff;line-height:5rem;left:0;}
.shop_btn_con .input{width:16rem;text-align:center;border:1px solid #9e9e9e;height: 5rem;}
.shop_btn_con input[value='-']{border-radius:0px;background-color:#eee;width: 5rem;height:100%;border-right:1px solid #9e9e9e;color: #999;font-size:2.5rem;padding-bottom: 0rem;}
.shop_btn_con input[value='+']{border-radius:0px;background-color:#eee;width: 5rem;height:100%;border-left:1px solid #9e9e9e;color: #999;font-size:2.5rem;padding-bottom: 0rem;}
.shop_btn_con input[name="num"]{border-radius:0px;text-align:center;vertical-align:middle;font-size:1.5625rem;width: 6rem;height: 5.375rem;border: none;font-size:2.5rem;color: #a3a1a1;}
.shop_btn_con .cat,.shop_btn_con .buy{width:14.375rem;height:5rem;font-size:1.875rem;text-align:center;color:#fff;}
.shop_btn_con .cat{background-color:#fec859;}
.shop_btn_con .buy{background-color:#ff755b;border:0;}
.shop_info_nav {line-height: 3.75rem;}
.shop_info_con{line-height: 150%;}

/*加减框*/
.plural{border-radius:5px;border:1px solid #9e9e9e;display: inline-block;height:3.125rem;overflow: hidden;position: absolute;bottom: 0.75rem;}
.plural input[value='-']{border-radius:0px;background-color:#eee;width: 3.5rem;height:3.125rem;border-right:1px solid #9e9e9e;color: #999;padding-bottom: 0rem;vertical-align: top;}
.plural input[value='+']{border-radius:0px;background-color:#eee;width: 3.5rem;height:3.125rem;border-left:1px solid #9e9e9e;color: #999;padding-bottom: 0rem;vertical-align: top;}
.plural input[name="num"]{border-radius:0px;text-align:center;vertical-align:top;width: 4rem;height:3.125rem;border: none;color: #a3a1a1;}
/*购物车*/
.shopcat{}
.shopcat .plural input[name="num"]{color: #fe7458;}
.shopcat ul li{border-bottom:1px solid #ddd;}
.shopcat ul li div{height: 10.5rem;float: left;overflow: hidden;padding: 0.875rem 0;}
.shopcat ul li .c{height: 10.5rem;width:5rem;overflow: hidden;}
.shopcat ul li .c p{display: table-cell;height: 10.5rem;width:3.75rem;padding-left:1.25rem;vertical-align: middle;}
.shopcat ul li .c p span{width: 1.625rem;height: 1.625rem;display: inline-block;border:0.125rem solid #646464;border-radius: 50%;}
.shopcat ul li .c p span.opt{width:6.375rem;height:1.875rem;border:none;border-radius:0;background: url(../images/qietu-mix.png) 0 -5.625rem no-repeat;background-size:100%;}
.shopcat ul li .i p{height: 10.5rem;width: 10.5rem;overflow: hidden;display: table-cell;vertical-align: middle;text-align: center;}
.shopcat ul li .i img{max-height:100%;width:100%;}
.shopcat ul li .n{width: 25.375rem;margin-left: 1.5rem;font-size: 1.5rem;position: relative;}
.shopcat_total{line-height: 5rem;height: 5rem;}
.shopcat_total_con{position: fixed;bottom: 0;left: 0;height: 5rem;line-height: 5rem;background-color: #fff;width: 100%;border-top:1px solid #ddd;}
.shopcat_total_con .submit .f_r{width: 14.375rem;text-align: center;background-color: #ff755b;color: #fff;}
.shopcat_total_con .submit>div:first-child,.shopcat_total_con .remove>div:first-child{text-indent: 1.25rem;}
.shopcat_total_con .remove .all .t{width: 3.6rem;overflow:hidden;}
.shopcat_total_con .remove .all .t span{width: 1.625rem;height: 1.625rem;display: inline-block;border:0.125rem solid #646464;border-radius: 50%;vertical-align: middle;margin-top: -0.625rem;}
.shopcat_total_con .remove .all .t span.opt{width:6.375rem;height:1.875rem;border:none;border-radius:0;background: url(../images/qietu-mix.png) 0 -5.625rem no-repeat;background-size:100%;}
.shopcat_total_con .remove .f_r{width: 14.375rem;text-align: center;background-color: #fa9250;color: #fff;}

/*订单*/
.order .site{height:7.5rem;padding:2.6875rem 1.25rem;border-bottom:0.625rem solid #ece7e4;}
.order .site a{width:100%;height:100%;display:block;}
.order .site .l{height:2.875rem;width:3.75rem;margin-top:2.3125rem;overflow: hidden;}
.order .site .l span{background:url(../images/plcg.png) 0px -19.25rem no-repeat;background-size:100%;height:2.875rem;width:5.9375rem;display: inline-block;}
.order .site .i{width:34.5rem;}
.order .site .r{width: 1.25rem;height:2.5rem;margin-top:3rem;overflow: hidden;}
.order .site .r span{background:url(../images/plcg.png) 0 -129.1875rem no-repeat;background-size:100%;width:5.9375rem;height:1.5rem;display: inline-block;} 
.order_l>div{padding:0 1.875rem;border-bottom:1px solid #ddd;}
.order_l .i .p img{width:9.4375rem;height:9.125rem;}
.order_l .i .n{width:20.1875rem;}
.order_l .i .price{text-align:right;width:10rem;}
.order_l .total{text-align:right;}
.order_foot{padding:1.75rem 2.5rem 1.5rem;border-bottom: 0.625rem solid #ece7e4;}
.order_foot p{padding:0.625rem 0px;}
.order_foot .leave textarea{height:6.25rem;width:100%;font-size:1em;}
.order_fix{height: 5rem;margin-top: }
.order_fix_con{position:fixed;width:100%;height:5rem;bottom:0;background:#fff;border-top:1px solid #ddd;line-height:5rem;left:0;}
.order_fix_con .total{padding-left:1.25rem;}
.order_fix_con .f_r{width:14.375rem;height:5rem;text-align:center;background:#ff755b;color:#fff;line-height:5rem;}
/*订单里地址*/
.order{overflow: hidden;}
.order .site_l{position:absolute;left:90%;width:100%;height:100%;z-index:3;top:0;overflow:hidden;}
.order .site_l_top{height:6.25rem;background-color:#fff;line-height:6.25rem;color:#ff745a;text-align:center;}
.order .site_l>div.f_l{width:15%;height:100%;background-color:rgba(66,66,66,0.2);}
.order .site_l>div.f_r{width:85%;overflow-y: scroll;}
.order .site_list{padding:0px 4.375rem;background-color:#fff;border-bottom:5px solid #f7f3e7;}
.order .site_l .now{background-color:#377812;color:#fff;}
.order .site_l .now .f7{color:#fff;}
.order .site_list p{padding:0.6rem 0;}

/*结束页面*/
.out{padding:3.125rem 0px 6.25rem;}
.out .over{width:6.375rem;height:6.5rem;background:url(../images/qietu-mix.png) 0px -17.75rem no-repeat;background-size: 100%;margin:0 auto;}
.out .error{width:95px;height:95px;background:url(../images/plcg.png) 0px -3873px no-repeat;margin:0 auto;}
.out .text{text-align:center;margin:3.125rem 0;}
.out .btn a{width:28.125rem;height:5rem;line-height:5rem;background-color:#fa8066;color:#fff;border-radius:5px;margin:1.125rem auto;text-align:center;display: block;}

/*用户界面】*/
.user .head_wrap{height:16.0625rem;background:-webkit-linear-gradient(top,#e0dccc,#d8c9af);}
.user .head_con{margin:auto;text-align:center;color:#443a2a;}
.user .head_con img{height:8.125rem;width:8.125rem;margin:auto;margin-top:1.8rem;margin-bottom:0.8rem;border:1px solid #fff;border-radius: 2px;}
.user .head_wrap .n{width:50%;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;margin: auto}
.user .main_wrap{background:#f4f4f4;}

.user .main_wrap .go_r_w{float:right}
/*.user .main_wrap .go_r{margin-top:2.59375rem;float:right;}*/
.user .flow{padding:0.625rem 3.125rem 0.9375rem;background:#fff;}
.user .flow div{float:left;width:25%;height:80px;text-align:center;}
.user .flow a{background:url(../images/plcg.png) 0px 0px no-repeat;display:inline-block;width:70px ;height:80px;}
.user .flow .f a{background-position:0px -605px;}
