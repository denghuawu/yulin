.user .flow .s a{background-position:0px -954px;}
.user .flow .p a{background-position:0px -1122px;}
.user .flow .t a{background-position:0px -1290px;}
.user .all,.user .history ,.user .site,.user .chamei{margin-top:0.625rem;height:6.625rem;line-height:6.625rem;background:#fff;padding:0 1.875rem}
.user .all .nav_left,.user .history  .nav_left,.user .site .nav_left,.user .chamei .nav_left{background:url(../images/plcg.png) 0px 0px no-repeat;width:59px;height:59px;display:inline-block;vertical-align:middle;margin-right:20px;}
.user .all .nav_left{background-position:0px 0px;}
.user .history  .nav_left{background-position:0px -158px;}
.user .site .nav_left{background-position:0px -308px;}
