.site .site_add div{width:21.5rem;height:4.625rem;border-radius:0.125rem;margin:auto;background-color:#fa8066;color:#fff;text-align:center;line-height:4.625rem;}


.addsite{padding:2.5rem 2.5rem;}
.addsite .r_select{height: 4rem;display:block;border-radius:0.25rem;border:1px solid #999;}
.addsite input[type="text"]{width:90%;border:none;background-color:#ebeaea;border-radius:0.5rem;line-height:2rem;padding:1rem 5%;height:2rem;font-size:1em;}
.addsite form>div{position: relative;margin: 0.5rem 0;}
.addsite select{position: absolute;display: block;width: 100%;padding-right:8%;border:none;background: none;text-indent: 1.25rem;height:4.25rem;appearance:none;-moz-appearance:none; -webkit-appearance:none;}
.addsite select,.addsite option{font-size:1em;}
.addsite textarea{width:94%;height:12rem;font-size:1em;padding:2rem 3%;border-radius:10px;}
.revampsite{border: 0.625rem solid #f7f3e7;border-width: 0.625rem 0px;line-height: 4.125rem;height: 4.125rem;}
.default_site{height: 4.125rem;}
.default_site_con{border-radius: 0.25rem;background: #fa8066;color: #fff;width: 23.5rem;text-align: center;position: fixed;bottom: 0rem;left: 0;right: 0;margin: auto;line-height: 4.125rem;height: 4.125rem;}

/*替换select 样式*/
.r_select{background: url(../images/next.png) no-repeat;background-position:98%;display:block;}

/*订单列表*/
.indent_top div{float:left;width:20%;text-align:center;padding:1rem 0;}
.indent_con li{padding:0rem 2.125rem;border-top:0.3125rem solid #f4f4f4;}
.indent_con li>div:first-child{line-height:3.25rem;height:3.25rem;border-bottom:1px solid #c6c6c6;}
.indent_con li>div.i{height:10.25rem;padding:1rem 0;overflow:hidden;border-bottom:1px solid #c6c6c6;line-height:1.875rem;font-size: 1.75rem;}
.indent_con li>div.i .p img{width:10.3125rem;height:10.3125rem;}
.indent_con li>div.i .n{max-width:21.25rem;}
.indent_con li>div.i .f_r{text-align:right;}
.indent_con li>div.b{line-height:2.875rem;padding-bottom: 10px;}
.indent_con li>div.b .f_r span{padding:0.375rem 1.3rem;background-color:#fa8066;color:#fff;display:inline-block;border-radius:5px;margin-top: -0.625rem;}
.indent_con li>div.b .f_r span.c{background-color:#b2b2b2;}

.indent_con .refund li div.b  .f_r span{padding: 0.5rem 1rem;line-height: 1em;margin: 0;}

/*登陆*/
.login{padding:4rem 5rem ;}
.login_con{text-align:center;}
.login_con span{margin-right:1.25rem;}
.login_con p:first-child{margin-bottom: 2.5rem;}
.login_con input[type='text'],.login_con input[type='password']{width:27.5rem;border:1px solid #888888;border-radius:5px;height:3.625rem;padding-left:1.25rem;background: #e6e6e6;border:none;}
.login_con input[type='submit']{width:19.25rem;height:4.375rem;border:none;background-color:#fa8066;color:#fff;border-radius:5px;}
.login_nav a{color:#838282;}

/*注册*/
.register{padding:6.25rem 5rem;}
.register_con{text-align:center;}
.register_con input[type='text'],.register_con input[type='password']{width:97%;border:1px solid #888888;border-radius:5px;height:3.625rem;padding-left:3%;margin-bottom:1.875rem;background: #f7f7f7;}
.register_con .code{}
.register_con .code input[placeholder='请输入验证码']{width:45%;float:left;}
.register_con .code input[type='button']{width:35%;border-radius:5px;height:3.75rem;background:#fff;border:1px solid #b0b0b0;float:right;font-size:1.4375rem;}
.register_con input[type='submit']{width:15rem;height:4rem;border:none;background-color:#fa8066;color:#fff;border-radius:5px;}
