.nav_top .re_left{background:url(../images/qietu-mix.png) 0px -0px no-repeat;position:absolute;top:0;bottom:0;left:2.5rem;margin:auto;height:2.5625rem;width:6.375rem;background-size: 100%;}
.nav_top .re_cat{background:url(../images/qietu-mix.png) 0px -169px no-repeat;position:absolute;top:0;bottom:0;height:66px;right:1.25rem;margin:auto;vertical-align:middle;width:67px;}
