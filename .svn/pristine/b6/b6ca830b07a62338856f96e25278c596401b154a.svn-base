<?php
//引入核心库文件
	include "2wmscsub.php";
	if ($_GET["url"]) {
		$url=$_GET;
	}else{
		exit('程序出错了！');
	}
	//定义纠错级别0
	$errorLevel = "L";
	//定义生成图片宽度和高度;默认为3
	$size = "4";
	//定义生成内容
	$content="{$url['url']}";
	if (!empty($content)) {
		//调用QRcode类的静态方法png生成二维码图片
		QRcode::png($content, false, $errorLevel, $size);
	}
	